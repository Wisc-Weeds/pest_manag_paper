@article{dorr2013,
  title = {A Comparison of Initial Spray Characteristics Produced by Agricultural Nozzles},
  author = {Dorr, Gary J. and Hewitt, Andrew J. and Adkins, Steve W. and Hanan, Jim and Zhang, Huichun and Noller, Barry},
  date = {2013-11-01},
  journaltitle = {Crop Protection},
